@model weddingPlanner.Models.Wedding
@{
    string weddingTitle = @Model.SpouseOne + " and " + @Model.SpouseTwo + "'s";
}
<div class="row">
    <div class="eight columns">
        <h3>@weddingTitle Wedding</h3>
    </div>
    <div class="two columns">
        <a asp-controller="Wedding" asp-action="Dashboard">Dashboard</a>
    </div>
    <div class="two columns">
        <a asp-controller="User" asp-action="Logout">Logout</a>
    </div>
</div>
<div class="row">
    <div class="one-half column">
        <h5>Date: @Model.Date.ToString("MMM d, yyyy")</h5>
        <h5>Guests:</h5>
        <ul>
            @foreach (var guestConnection in @Model.Guests){
                <li>@guestConnection.Guest.first_name @guestConnection.Guest.last_name</li>
            }
        </ul>
    </div>
    <div class="one-half column">
        <img src=@ViewBag.mapstring alt="Wedding Map">
    </div>
</div>